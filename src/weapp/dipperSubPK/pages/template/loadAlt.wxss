.load-alt {
  position: fixed;
  top: 0rpx;
  width: 100vw;
  height: 100vh;
  z-index: 7777;
  left: 0rpx;
}

.alert-wiw-mark {
  width: 100%;
  height: 100%;
  opacity: 0.6;
  background: #000;
}

.alert-wiw-box {
  position: absolute;
  top: 50%;
  /* margin-top: -140rpx; */
  margin: -140rpx 122rpx 0rpx 122rpx;
  width: 506rpx;
  background: #fff;
  border-radius: 8rpx;
  text-align: center;
}

.alert-wiw-box-conten-box {
  padding: 56rpx 52rpx;
  overflow: hidden;
}

.alert-wiw-box-conten {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #3e4a59;
  white-space: normal;
  text-align: left;
}

.alert-wiw-box-clk {
  border-top: 2rpx solid #ecf0f4;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  text-align: center;
  color: #47b3e3;
  padding-top: 24rpx;
  padding-bottom: 24rpx;
}

/* ～～～～～～～～～带电话弹窗～～～～～～～～～～～ */

.show-modal {
  position: absolute;
  top: 50%;
  margin: -300rpx 122rpx 0rpx 80rpx;
  width: 600rpx;
  background: #fff;
  border-radius: 8rpx;
  text-align: center;
}

.show-modal-close {
  width: 36rpx;
  height: 36rpx;
  z-index: 3;
  display: inline-block;
}

.show-modal-box {
  padding: 0rpx 52rpx 60rpx 52rpx;
}

.show-modal-box-title {
  font-weight: 500;
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #001a4a;
  padding-bottom: 40rpx;
}

.show-modal-box-content {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #39487f;
  white-space: pre-wrap;
  text-align: left;
}

.show-modal-btn {
  margin-top: 76rpx;
  display: flex;
  height: 88rpx;
}

.show-modal-btn cover-view {
  flex: 1;
  text-align: center;
  border-radius: 4rpx;
  height: 88rpx;
  line-height: 88rpx;
  align-items: center;
  font-weight: 500;
}

.show-modal-btn cover-view:first-child {
  background-color: #001a4a;
  color: #fff29c;
  margin-right: 10rpx;
}

.show-modal-btn cover-view:last-child {
  background-color: #f8d14b;
  color: #001a4a;
  margin-left: 10rpx;
}

/* 您的购房需求 */

.buy-house {
  position: absolute;
  top: 50%;
  margin: -454rpx 122rpx 0rpx 80rpx;
  width: 600rpx;
  background: #fff;
  border-radius: 8rpx;
}

.buy-house-box {
  padding: 0rpx 52rpx 60rpx 52rpx;
}

.buy-house-box-title {
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  line-height: 56rpx;
  color: #001a4a;
  text-align: center;
  margin-bottom: 40rpx;
}

.buy-house-box-content {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #39487f;
  white-space: pre-wrap;
  margin-bottom: 27rpx;
}

.buy-house-box-input {
  height: 72rpx;
  display: flex;
  align-items: center;
  margin-bottom: 16rpx;
}

.input-box {
  border: 1rpx solid #b5b5b5;
  border-radius: 4rpx;
  width: 210rpx;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 32rpx;
  box-sizing: border-box;
  text-align: center;
  color: #001a4a;
  overflow: hidden;
  position: relative;
}

.input-box cover-view {
  font-size: 32rpx;
  border-radius: 4rpx;
  width: 210rpx;
  line-height: 72rpx;
  text-align: center;
  color: #001a4a;
}

.buy-house-box-input input {
  border: 1rpx solid #b5b5b5;
  border-radius: 4rpx;
  width: 210rpx;
  height: 72rpx;
  font-size: 32rpx;
  box-sizing: border-box;
  text-align: center;
  color: #001a4a;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
}

.buy-house-box-input .label-title {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #001a4a;
  display: inline-block;
  width: 128rpx;
  margin-right: 78rpx;
}

.buy-house-box-input .label-gray {
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #707798;
  width: 64rpx;
  margin-left: 16rpx;
  text-align: right;
}

.buy-house-box-selct {
  margin-top: 14rpx;
  display: flex;
  position: relative;
}

.buy-house-box-selct-name {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #001a4a;
  display: inline-block;
  width: 128rpx;
  margin-right: 134rpx;
}

.buy-house-box-selct-content {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.buy-house-box-selct-content-item {
  display: flex;
  align-items: center;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  height: 45rpx;
  color: #001a4a;
}

.buy-house-box-selct-content  cover-view:nth-child(1){
 margin-right: 46rpx;
}

.buy-house-box-selct-content  cover-view:nth-child(2){
  position: relative;
  right: 0rpx;
}

.buy-house-box-selct-content  cover-view:nth-child(3){
 margin-right: 66rpx;
}


.buy-house-box-selct-content cover-view cover-image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 16rpx;
  margin-left: 2rpx;
}

.buy-house-box-footer {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  line-height: 40rpx;
  height: 40rpx;
  overflow: hidden;
  color: #707798;
  margin-top: 16rpx;
  display: flex;
 justify-content: flex-end;
}

.buy-house-box-footer  cover-view{
  display: inline-block
}
.buy-house-box-footer  cover-image {
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
  margin-right: -4rpx;
}

.buy-house-btn {
  margin-top: 32rpx;
  display: flex;
  height: 88rpx;
}

.buy-house-btn cover-view {
  flex: 1;
  text-align: center;
  border-radius: 4rpx;
  height: 88rpx;
  line-height: 88rpx;
  align-items: center;
  font-size: 32rpx;
  box-sizing: border-box;
}

.buy-house-btn cover-view:first-child {
  border: 1rpx solid #001a4a;
  color: 001a4a;
  margin-right: 10rpx;
}

.buy-house-btn cover-view:last-child {
  background: #001a4a;
  color: #fff29c;
  margin-left: 10rpx;
}

.input-box  .input-placeholder {
  color: #b5b5b5;
}

.show-modal-close-box {
  text-align: right;
  padding-top: 24rpx;
  padding-right: 24rpx;
}

}

.search-result-btn .tel,
.search-result-btn .house {
  color: #fff;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 28rpx;
  border-radius: 8rpx;
}

.search-result-btn .tel {
  background-color: #00c0eb;
}

.search-result-btn .house {
  margin-left: 20rpx;
  background-color: #fa5f35;
}

.result-original-point {
  width: 6rpx;
  height: 6rpx;
  border-radius: 50%;
  background: #3e4a59;
  display: inline-block;
  margin-right: 8rpx;
}


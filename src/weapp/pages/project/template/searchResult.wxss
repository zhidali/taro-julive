.search-result {
  overflow: hidden;
}

.search-result-normal {
  display: flex;
  overflow: hidden;
  margin: 52rpx 0rpx;
}

.search-result-normal-left image {
  display: block;
  height: 300rpx;
  width: 268rpx;
  background-size: cover;
  background-image: url('http://imgs.julive.com/l?p=eyJpbWdfcGF0aCI6IlwvbWluaW1ncy9pbWctcHJvamVjdC1kaWxpLnBuZz93YXRlcm1hcmssdF8wIn0gICA=');
}

.search-result-normal-right .name {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  line-height: 44rpx;
  color: #3e4a59;
  margin-top: 20rpx;
}

.search-result-character {
  margin-top: 16rpx;

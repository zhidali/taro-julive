.project-list-cell {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  background-color: white;
  width: 100%;
  position: relative;
  padding-bottom: 40rpx;
  border-bottom: 1rpx solid #ecf0f4;
  margin-top: 40rpx;
}

.project-list-hover-cell {
  background-color: #efefef;
}

.project-list-top-view {
  display: flex;
  flex-direction: row;
  width: 100%;
  justify-content: flex-start;
  position: relative;
}

.dipper-project-image {
  width: 224rpx;
  height: 168rpx;
  background-color: #efefef;
  border-radius: 8rpx;
  display: block;
}

.project-sale-activity {
  position: absolute;
  width: 125rpx;
  height: 50rpx;
  left: 10rpx;
  top: 10rpx;
}

.project-cell-info-view {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  margin-left: 16rpx;
  flex: 1;
}

.project-info-name-view {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  line-height: 45rpx;
  height: 45rpx;
}

.project-info-name {
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  font-family: PingFangSC-Semibold;
  font-size: 32rpx;
  color: #3e4a59;
  line-height: 45rpx;
  height: 45rpx;
}

.project-sale-status {
  background: #0ad487;
  border-radius: 4rpx;
  color: white;
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  line-height: 32rpx;
  height: 32rpx;
  padding: 0rpx 8rpx;
  margin-right: 0rpx;
  white-space: nowrap;
  flex-wrap: nowrap;
}

.project-sale-out {
  color: white;
  background: #aaa;
}

.project-list-price {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
  margin-top: 7rpx;
  margin-bottom: 7rpx;
  line-height: 40rpx;
}

.project-list-price-label {
  font-family: PingFangSC-Semibold;
  font-size: 22rpx;
  color: #666;
  margin-right: 10rpx;
}

.project-list-info-unit {
  font-size: 28rpx;
  color: #fa5f35;
  font-family: PingFangSC-Medium;
}

.project-list-info-unit-rigth {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #77808a;
  display: inline-block;
  margin-left: 10rpx;
}

.project-list-info-deatil {
  font-family: PingFang-SC-Regular;
  font-size: 12px;
  line-height: 14px;
  height: 14px;
  color: #77808a;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  margin-bottom: 9rpx;
}

.project-list-tags {
  height: 36rpx;
  /* white-space: nowrap; */
  overflow: hidden;
  position: relative;
}

.project-list-tags  text {
  /* font-family: PingFangSC-Regular; */
  margin-right: 8rpx;
  display: inline-block;
  box-sizing: border-box;
  height: 36rpx;
  line-height: 36rpx;
  padding: 0px 8rpx;
  font-size: 22rpx;
  position: relative;
  top: -8rpx;
}

.project-list-tag-blue {
  color: #00c0eb;
  background: #e1faff;
}

.project-list-tag-gray {
  color: #77808a;
  background: #f3f6f9;
}

.project-list-dsc {
  width: 100%;
  padding: 20rpx;
  background: #f4f8f9;
  margin-top: 26rpx;
  position: relative;
  box-sizing: border-box;
  border-radius: 8rpx;
  word-wrap: break-word;
  word-break: normal;
}

.project-triangle {
  width: 20rpx;
  height: 10rpx;
  position: absolute;
  top: -10rpx;
  left: 85rpx;
}

.project-list-dsc-title {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #3e4a59;
  line-height: 44rpx;
  margin-bottom: 20rpx;
}

.project-list-dsc-content {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #3e4a59;
  line-height: 44rpx;
}

.dsc-content-unfold {
  color: #00c0eb;
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
}

.line-clamp {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}

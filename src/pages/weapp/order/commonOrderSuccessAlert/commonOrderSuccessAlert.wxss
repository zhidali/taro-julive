.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 1000;
}
.modal-dialog {
  width: 88vw;
  overflow: hidden;
  position: fixed;
  top: 20%;
  left: 6vw;
  right: 6vw;
  z-index: 9999;
  background: #fff;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}

.modal-dialog-red {
  width: 88vw;
  height: 65vw;
  overflow: hidden;
  position: fixed;
  top: 20%;
  left: 6vw;
  right: 6vw;
  z-index: 9999;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  background-size: cover;
  background-image: url('http://imgs.julive.com/l?p=eyJpbWdfcGF0aCI6IlwvbWluaW1ncy9jb21tb25PcmRlclN1Y2Nlc3NBbGVydC5wbmcifQ==');
}

.modal-icon {
  width: 50px;
  height: 50px;
  margin-top: 88rpx;
}

.modal-icon-close {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 16rpx;
  right: 16rpx;
}
.modal-title {
  padding-top: 40rpx;
  font-family: PingFang-SC-Bold;
  font-size: 40rpx;
  color: #3e4a59;
  text-align: center;
}

.modal-title-yellow {
  color: #fffc00;
  margin-top: 80rpx;
}

.modal-content {
  padding: 40rpx 52rpx 84rpx 52rpx;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.modal-content-text {
  font-family: PingFang-SC-Medium;
  font-size: 28rpx;
  color: #3e4a59;
  text-align: center;
  line-height: 40rpx;
  width: 100%;
  min-width: 100%;
  word-break: break-all;
  word-wrap: break-word;
  white-space: pre-line;
}

.modal-footer {
  box-sizing: border-box;
  padding: 0 52rpx;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

.btn-confirm {
  width: 100%;
  font-family: PingFang-SC-Bold;
  color: white;
  background-color: #00c0eb;
  border-radius: 4px;
  text-align: center;
  align-items: center;
  margin-bottom: 88rpx;
  font-size: 32rpx;
  height: 88rpx;
  line-height: 88rpx;
}

.btn-confirm-yellow {

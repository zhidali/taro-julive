.tip-pop {
  position: fixed;
  top: 0rpx;
  width: 100vw;
  height: 100vh;
  z-index: 9999;
  left: 0rpx;
  background-color: rgba(0, 0, 0, 0.4);
}
.tip-wiw-box {
  width: 506rpx;
  position: fixed;
  overflow: hidden;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 10000;
  margin: 0;
  background: #fff;
  border-radius: 8rpx;
  text-align: center;
}
.tip-wiw-box-conten-box {
  padding: 56rpx 52rpx;
  overflow: hidden;
}

.tip-wiw-box-conten {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  font-family: PingFangSC-Regular, PingFang SC;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #031a1f;
  white-space: normal;
  text-align: left;
}

.tip-wiw-box-clk {
  border-top: 2rpx dashed #ecf0f4;
  font-family: PingFangSC-Regular, PingFang SC;
  font-size: 32rpx;
  text-align: center;
  color: rgba(0, 192, 235, 1);
  padding-top: 24rpx;
  padding-bottom: 24rpx;
}

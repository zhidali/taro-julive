.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 88vw;
  overflow: hidden;
  position: fixed;
  z-index: 9999;
  background: #fff;
  border-radius: 10rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.modal-dialog-red {
  width: 88vw;
  height: 70vw;
  left: 6vw;
  right: 6vw;
  overflow: hidden;
  position: fixed;
  top: 20%;
  z-index: 9999;
  border-radius: 10px;
  background-image: url('http://imgs.julive.com/l?p=eyJpbWdfcGF0aCI6IlwvbWluaW1ncy9sb2dpbi5wbmcifQ==');
  background-size: cover;
}

.icon-cancel-view {
  position: relative;
  width: 30px;
  height: 30px;
  float: right;
}

.icon-cancel {
  width: 30px;
  height: 30px;
}
.icon-cancel-view-new {
  position: absolute;
  top: 16rpx;
  right: 16rpx;
}
.icon-cancel-new {
  height: 36rpx;
  width: 36rpx;
}

.icon-cancel-red {
  width: 28px;
  height: 28px;
  float: right;
